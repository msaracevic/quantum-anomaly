{
  "name": "quantum_anomaly_frontend",
  "version": "0.2.1",
  "dependencies": {
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.11"
  },
  "author": "Miroslav Saracevic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msaracevic/quantum_anomaly/issues"
  },
  "proxy": "http://localhost:3000/",
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css-production && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css-production": "node-sass --include-path ./src --include-path ./node_modules src/ -o public/ --output-style compressed",
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o public/",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o public/ --watch --recursive"
  }
}
